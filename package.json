{
  "name": "deploy-clouddeploy",
  "version": "0.6.0",
  "description": "Github Action: Deploy to Google Cloud Deploy",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc && ncc build",
    "lint": "eslint src/ --ext .ts,.tsx",
    "format": "prettier --write **/*.ts",
    "test": "mocha -r ts-node/register -t 150s 'tests/unit/*.test.ts' --exit",
    "e2e-tests": "mocha -r ts-node/register -t 150s 'tests/e2e.test.ts' --exit",
    "package": "ncc build --source-map --license licenses.txt",
    "all": "npm run build && npm run format && npm run lint && npm run package"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gcp-cloud-deploy-ecosystem/deploy-clouddeploy.git"
  },
  "keywords": [
    "actions",
    "google",
    "cloud deploy",
    "cloud",
    "deploy",
    "skaffold",
    "containers"
  ],
  "author": "gcp-cloud-deploy-ecosystem",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/gcp-cloud-deploy-ecosystem/deploy-clouddeploy/issues"
  },
  "homepage": "https://github.com/gcp-cloud-deploy-ecosystem/deploy-clouddeploy#readme",
  "dependencies": {
    "@actions/core": "^1.5.0",
    "@actions/exec": "^1.1.0",
    "@actions/http-client": "^1.0.11",
    "@actions/tool-cache": "^1.7.1",
    "@lifeomic/attempt": "^3.0.0",
    "@types/tmp": "^0.2.1",
    "core-util-is": "^1.0.3",
    "fs": "0.0.2",
    "google-auth-library": "^7.9.2",
    "googleapis": "^85.0.0",
    "tmp": "^0.2.1",
    "typed-rest-client": "^1.8.5",
    "yaml": "^1.10.0"
  },
  "devDependencies": {
    "@types/chai": "^4.2.21",
    "@types/js-yaml": "^4.0.3",
    "@types/lodash": "^4.14.173",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.9.1",
    "@types/sinon": "^10.0.2",
    "@types/uuid": "^8.3.1",
    "@typescript-eslint/eslint-plugin": "^4.31.0",
    "@typescript-eslint/parser": "^4.31.0",
    "@vercel/ncc": "^0.31.1",
    "chai": "^4.2.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "lodash": "^4.17.21",
    "mocha": "^10.0.0",
    "prettier": "^2.4.1",
    "sinon": "^11.1.2",
    "ts-node": "^10.2.1",
    "typescript": "^4.3.5"
  }
}
